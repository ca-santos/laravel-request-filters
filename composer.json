{
  "name": "caue-santos/laravel-request-filters",
  "description": "A laravel request filters",
  "license": "MIT",
  "keywords": [
    "laravel",
    "request",
    "filters"
  ],
  "homepage": "https://github.com/caueSantos/laravel-request-filters",
  "type": "library",
  "authors": [
    {
      "name": "Caue Santos",
      "email": "cauesantosre4@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8",
    "laravel/framework": "^8.0|^9.0|^10.10",
    "caue-santos/auto-class-discovery": "dev-master"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "CaueSantos\\LaravelRequestFilters\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CaueSantos\\LaravelRequestFilters\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CaueSantos\\LaravelRequestFilters\\RequestFiltersServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "path",
      "url": "../auto-class-dicovery",
      "options": {
        "symlink": true
      }
    }
  ]
}
